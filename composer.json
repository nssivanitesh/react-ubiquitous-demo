{
    "name": "nssivanitesh/react-ubiquitous-composer",
    "description": "Laravel Composer package providing standardized DTOs and helper functions for the react-ubiquitous UI Config API",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "dto", "react-ubiquitous", "ui-config"],
    "authors": [
        {
            "name": "nssivanitesh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "ReactUbiquitous\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactUbiquitous\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReactUbiquitous\\ReactUbiquitousServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
